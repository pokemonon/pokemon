  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/packages/knife/src/common/function/memoize.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "current file",
      "program": "${relativeFile}",
      "runtimeExecutable": "ts-node",
      "console": "integratedTerminal"
    }
  ]
}
