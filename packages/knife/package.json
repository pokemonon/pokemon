{
    "name": "@pokemonon/knife",
    "version": "1.2.0-beta.2",
    "main": "dist/lib/common/index.js",
    "module": "dist/es/common/index.js",
    "exports": {
        "./node": {
            "require": "./dist/lib/node/index.js",
            "import": "./dist/es/node/index.js",
            "default": "./dist/es/node/index.js"
        },
        "./browser": {
            "require": "./dist/lib/browser/index.js",
            "import": "./dist/es/browser/index.js"
        },
        "./types": {
            "require": "./dist/lib/types/index.js",
            "import": "./dist/es/types/index.js"
        },
        ".": {
            "require": "./dist/lib/common/index.js",
            "import": "./dist/es/common/index.js"
        },
        "./*": "./*"
    },
    "license": "MIT",
    "sideEffects": false,
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "manage": "tsx scripts/manager/index.ts",
        "dev": "npm run build -- --watch",
        "build": "rm -rf dist && packbag rollup",
        "refresh": "ts-node scripts/initEntry.ts",
        "docs": "npm run build && tsx scripts/docs",
        "test": "vitest"
    },
    "devDependencies": {
        "@packbag/cli": "0.0.1-alpha.3",
        "@packbag/cli-rollup": "0.0.1-alpha.3",
        "@packbag/cli-rollup-plugin-common": "0.0.1-alpha.3",
        "@pokemonon/temanager": "0.0.2-alpha.0",
        "@types/inquirer": "^9.0.2",
        "@types/listr": "^0.14.3",
        "better-docs": "^2.3.2",
        "eslint-plugin-tsdoc": "^0.2.11",
        "inquirer": "^9.1.4",
        "jsdoc": "^3.6.6",
        "jsdoc-plugin-typescript": "^2.0.6",
        "rimraf": "^3.0.2",
        "tsup": "^4.8.21",
        "vite-tsconfig-paths": "^3.5.1",
        "vitest": "^0.24.1"
    },
    "dependencies": {
        "chalk": "^4.1.1",
        "commander": "^9.4.1",
        "execa": "^5.0.0",
        "find-up": "^5.0.0",
        "fs-extra": "^10.0.0",
        "listr": "^0.14.3",
        "strip-ansi": "^6.0.0"
    },
    "peerDependencies": {
        "typescript": "~4.7.4"
    }
}
