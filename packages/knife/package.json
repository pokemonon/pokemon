{
    "name": "@pokemonon/knife",
    "version": "1.0.0-beta.0",
    "main": "dist/common.js",
    "module": "dist/esm/common.js",
    "license": "MIT",
    "sideEffects": false,
    "scripts": {
        "build": "rimraf dist && npm run build:common && npm run build:node && npm run build:browser",
        "build:common": "tsup src/common/common.ts --format esm,cjs --legacy-output --dts",
        "build:node": "tsup src/node/node.ts --format esm,cjs --legacy-output --dts",
        "build:browser": "tsup src/browser/browser.ts --format esm,cjs --legacy-output --dts",
        "refresh": "ts-node scripts/initEntry.ts",
        "doc:dev": "vitepress dev doc",
        "doc": "jsdoc -c jsdoc.json"
    },
    "devDependencies": {
        "better-docs": "^2.3.2",
        "eslint-plugin-tsdoc": "^0.2.11",
        "jsdoc": "^3.6.6",
        "jsdoc-plugin-typescript": "^2.0.6",
        "rimraf": "^3.0.2",
        "tsup": "^4.8.21"
    },
    "dependencies": {
        "typescript": "^4.2.4"
    },
    "gitHead": "bb58ecff2a5a5486724b422154c241500716ad85"
}
